@model IEnumerable<DoAnWebTruyenTranh.Models.Chapter>
@{
    ViewBag.Title = "XemTruyen";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>AAAAAAAAAAAAAAA</h2>
<div class="row">
    <div class="caption">
         @foreach (var item in Model.OrderByDescending(x=>x.CSequence).ToList())
         { 
             <div class="row">
                 <a href="@Url.Action("XemChapter","Home",new {id=item.IDChapter})">Chapter: @item.CSequence</a>
             </div>
         }
    </div>
</div>
